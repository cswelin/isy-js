{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended"
        
    ],
    "jsRules": {},
    "rules": {"no-return-await":true,
        "member-ordering":true,
        "variable-name":false,
        "triple-equals":true,
        "indent":[true,"tabs",2],
        "max-classes-per-file":false,
        "max-line-length":false,
        "quotemark":[true,"single"],
        "prefer-template": true,
        "interface-name": false,
        "trailing-comma": false,
        "semicolon":true
    },
    
    "rulesDirectory": []
}